{
  "presets": [
    [
      "@nrwl/react/babel",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    ["styled-components", { "pure": true, "ssr": true }],
    [
      "module-resolver",
      {
        "alias": {
          "^react-native$": "react-native-web"
        }
      }
    ],
    ["react-native-web", { "commonjs": true }],
    ["react-native-reanimated/plugin"]
  ]
}
